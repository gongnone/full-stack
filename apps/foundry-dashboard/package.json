{
  "name": "foundry-dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:worker": "wrangler dev --remote",
    "dev:worker:local": "wrangler dev --local",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "typecheck:build": "tsc --noEmit --project tsconfig.build.json",
    "test": "NODE_OPTIONS='--max-old-space-size=8192' vitest run",
    "test:watch": "NODE_OPTIONS='--max-old-space-size=8192' vitest",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=8192' vitest run --coverage",
    "test:ui": "NODE_OPTIONS='--max-old-space-size=8192' vitest run src/components/ui/",
    "test:hub-wizard": "NODE_OPTIONS='--max-old-space-size=8192' vitest run src/components/hub-wizard/",
    "test:hubs": "NODE_OPTIONS='--max-old-space-size=8192' vitest run src/components/hubs/",
    "test:clients": "NODE_OPTIONS='--max-old-space-size=8192' vitest run src/components/clients/",
    "test:exports": "NODE_OPTIONS='--max-old-space-size=8192' vitest run src/components/exports/",
    "test:review": "NODE_OPTIONS='--max-old-space-size=8192' vitest run src/components/review/",
    "test:brand-dna": "NODE_OPTIONS='--max-old-space-size=8192' vitest run src/components/brand-dna/",
    "test:layout": "NODE_OPTIONS='--max-old-space-size=8192' vitest run src/components/layout/",
    "test:settings": "NODE_OPTIONS='--max-old-space-size=8192' vitest run src/components/settings/",
    "test:analytics": "NODE_OPTIONS='--max-old-space-size=8192' vitest run src/components/analytics/",
    "test:batch": "pnpm test:ui && pnpm test:hub-wizard && pnpm test:hubs && pnpm test:clients && pnpm test:exports && pnpm test:review && pnpm test:brand-dna && pnpm test:layout && pnpm test:settings && pnpm test:analytics",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:story-1.3": "playwright test story-1.3",
    "stage:deploy": "pnpm build && wrangler deploy --env stage",
    "production:deploy": "pnpm build && wrangler deploy --env production",
    "deploy:stage": "pnpm build && wrangler deploy --env stage",
    "deploy:production": "pnpm build && wrangler deploy --env production"
  },
  "dependencies": {
    "@hono/trpc-server": "^0.4.1",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.0",
    "@radix-ui/react-select": "^2.1.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.0",
    "@radix-ui/react-tooltip": "^1.1.0",
    "@repo/foundry-core": "workspace:*",
    "@tanstack/react-query": "^5.60.0",
    "@tanstack/react-router": "^1.82.0",
    "@trpc/client": "^11.0.0-rc.648",
    "@trpc/react-query": "^11.0.0-rc.648",
    "@trpc/server": "^11.0.0-rc.648",
    "better-auth": "^1.4.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "hono": "^4.6.0",
    "kysely": "^0.28.9",
    "kysely-d1": "^0.4.0",
    "lucide-react": "^0.460.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.6.0",
    "tailwind-merge": "^2.5.4",
    "zod": "^3.24.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@cloudflare/workers-types": "^4.20241127.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/router-plugin": "^1.82.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.20",
    "jsdom": "^26.1.0",
    "playwright": "^1.57.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.8.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.16",
    "wrangler": "^4.56.0"
  }
}
